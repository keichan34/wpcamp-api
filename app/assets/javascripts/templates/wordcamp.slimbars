.wordcamp-tile.col-lg-3.col-md-6.col-xs-6 data={ id: "{{id}}" }
  a.wordcamp-tile-inner href="{{url}}"
    h5.title
      | {{title}}
    .period
      | {{ dates.start }}
      | {{#if dates.end}}
        ~ {{ dates.end }}
      | {{/if}}
    img.img-responsive.thumbnail-image src="{{thumbnail.thumb}}"
